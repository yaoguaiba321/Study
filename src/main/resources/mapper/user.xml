<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.server.dao.UserDao">
    <select id="selectUser" parameterType="java.lang.String" resultType="com.study.server.entity.User">
        select * from users where account='${account}';
    </select>
    <insert id="insertUser" parameterType="com.study.server.entity.User">
        insert into users (account, nickname) values (#{account}, #{nickname});
    </insert>
</mapper>